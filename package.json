{
  "name": "gulp-boilerplate",
  "version": "0.0.0",
  "description": "Generic gulp boilerplate for frontend application",
  "scripts": {
    "start": "gulp serve --env dev",
    "dist": "gulp build --env dist",
    "test": "gulp test",
    "postinstall": "tsd purge && tsd reinstall --clean --overwrite && tsd link && tsd rebundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tarlepp/gulp-boilerplate.git"
  },
  "keywords": [
    "gulp",
    "boilerplate",
    "typescript"
  ],
  "author": "Tarmo Leppanen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tarlepp/gulp-boilerplate/issues"
  },
  "homepage": "https://github.com/tarlepp/gulp-boilerplate#readme",
  "devDependencies": {
    "connect-livereload": "0.5.3",
    "express": "4.13.3",
    "gulp": "3.9.0",
    "tiny-lr": "0.2.1",
    "ts-node": "0.5.1",
    "tsd": "0.6.5",
    "typescript": "1.6.2",
    "typescript-register": "1.1.0",
    "yargs": "3.30.0"
  }
}
